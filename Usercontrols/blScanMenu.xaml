<UserControl x:Class="BoothLeads.Usercontrols.blScanMenu"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="151" d:DesignWidth="243">
    
    <Grid x:Name="LayoutRoot" Background="WhiteSmoke">
        <Button Content="Scan Another" Height="72" HorizontalAlignment="Left" Margin="1,74,0,0" Name="btnScanAgain" VerticalAlignment="Top" Width="243" FontSize="20" Click="btnScanAgain_Click" Foreground="Black" />
        <Button Content="Verify Lead" Height="72" HorizontalAlignment="Left" Margin="1,22,0,0" Name="btnVerifyLead" VerticalAlignment="Top" Width="243" FontSize="20" Click="btnVerifyLead_Click" Foreground="Black" />
        <TextBlock Height="30" HorizontalAlignment="Center" Margin="13,3,11,0" Name="txtBlock" Text="booth admin" VerticalAlignment="Top" Width="219" TextAlignment="Center" Foreground="Black" />
    </Grid>
</UserControl>
